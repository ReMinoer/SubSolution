<?xml version="1.0" encoding="utf-8"?>
<SubSolutionConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                          xsi:noNamespaceSchemaLocation="SubSolutionConfiguration.xsd">
    <Root RemoveUniqueItemFolders="true"
          CollapseEmptyFolders="true"
          ConflictProjectsOutputDirectory=".Conflicts">
        <Folder Name="FolderName"
                RemoveUniqueItemFolders="true"
                CollapseEmptyFolders="true"
                ConflictProjectsOutputDirectory=".Conflicts">
            <SubSolutions CreateRootFolder="true" CreateFolders="true">
                <MatchingPath Path="**/*.custom.csproj" />
            </SubSolutions>
            <Projects />
        </Folder>
        <SubSolutions CreateRootFolder="true" CreateFolders="true" />
        <Projects />
    </Root>
    <ConfigurationBindings UseDefaultBindings="true">
        <Configuration Project="Debug" Solution="Debug" />
        <Configuration Project="Release" Solution="Release" />
        <Platform Project="Any CPU" Solution="Any CPU" />
        <Platform Project="x86" Solution="x86" />
        <Platform Project="x64" Solution="x64" />
        <Platform Project="Any CPU" Solution="x86" />
        <Platform Project="Any CPU" Solution="x64" />
    </ConfigurationBindings>
</SubSolutionConfiguration>